B	test.cpp	/^class B$/;"	c	file:
B::a	test.cpp	/^	int a;$/;"	m	class:B	file:	access:public
B::foo	test.cpp	/^	void foo(int data) const$/;"	f	class:B	access:public	signature:(int data) const
B::foo	test.cpp	/^	void foo(int data)$/;"	f	class:B	access:public	signature:(int data)
a	test.cpp	/^	int a;$/;"	m	class:B	file:	access:public
foo	test.cpp	/^	void foo(int data) const$/;"	f	class:B	access:public	signature:(int data) const
foo	test.cpp	/^	void foo(int data)$/;"	f	class:B	access:public	signature:(int data)
main	test.cpp	/^int main(void)$/;"	f	signature:(void)
